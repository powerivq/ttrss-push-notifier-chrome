{
   "background": {
      "scripts": [ "const.js", "jquery-2.1.3.min.js", "main.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
   },
   "description": "Display updates on TTRSS",
   "icons": {
      "128": "icon.png",
      "19": "icon.png",
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "TTRSS Push Notifier",
   "permissions": [ "background", "notifications", "storage", "gcm", "clipboardWrite", "<all_urls>" ],
   "short_name": "TTRSS Push Notifier",
   "version": "0.9.0"
}
